{
  "expo": {
    "name": "HuntFish CA",
    "slug": "hf-ca",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/_default/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/_default/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F0F3F4"
    },
    "androidStatusBar": {
      "backgroundColor": "#F0F3F4",
      "barStyle": "dark-content"
    },
    "assetBundlePatterns": [
      "./src/assets/_default/*"
    ],
    "ios": {
      "requireFullScreen": true,
      "supportsTablet": true,
      "bundleIdentifier": "com.aspiraconnect.hf.ca",
      "buildNumber": "1.0.0.1",
      "userInterfaceStyle": "light",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "UIStatusBarStyle": "UIStatusBarStyleLightContent",
        "MinimumOSVersion": "13.0",
        "NSLocationWhenInUseUsageDescription": "This is used to search for retail licensing locations close to your location.",
        "NSFaceIDUsageDescription": "Enabling Face ID allows you quick and secure access to your account."
      }
    },
    "android": {
      "package": "com.aspiraconnect.hf.ca",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/_default/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "USE_BIOMETRIC",
        "USE_FINGERPRINT"
      ]
    },
    "web": {
      "favicon": "./src/assets/_default/images/favicon.png"
    }
  }
}
